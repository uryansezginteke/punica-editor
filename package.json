{
  "name": "punica-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "clean": "rm -rf dist",
    "up": "yarn upgrade-interactive --latest",
    "lint": "scripty",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\""
